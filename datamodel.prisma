type User {
  id: ID! @unique
  firstName: String!
  lastName: String!
  email: String!
  phone: String!
}

type Project {
  id: ID! @unique
  title: String!
  description: String!
  longDescription: String!
  image: String
  largeImage: String
  amount: Int!
  createdAt: DateTime!
  updatedAt: DateTime!
  # user: User!
}
